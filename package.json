{
  "name": "maptiler-geocoding-control",
  "private": true,
  "version": "0.0.31",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && FLAVOUR=leaflet vite build && tsc --outDir dist --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "files": [
    "dist",
    "src/lib"
  ],
  "exports": {
    "./leaflet": {
      "import": "./dist/leaflet.js",
      "require": "./dist/leaflet.umd.cjs"
    },
    "./maplibregl": {
      "import": "./dist/maplibregl.js",
      "require": "./dist/maplibregl.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./src/lib/Geocoder.svelte": {
      "import": "./src/lib/Geocoder.svelte",
      "require": "./src/lib/Geocoder.svelte"
    }
  },
  "types": "./dist/lib/index.d.ts",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/leaflet": "^1.9.0",
    "prettier-plugin-svelte": "^2.8.0",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2"
  },
  "peerDependencies": {
    "leaflet": "^1.9.2",
    "maplibre-gl": "> 1.14.0"
  },
  "peerDependenciesMeta": {
    "leaflet": {
      "optional": true
    },
    "maplibre-gl": {
      "optional": true
    }
  }
}
